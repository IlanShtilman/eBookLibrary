@model IEnumerable<LibraryProject.Models.User>

<h2>Users List</h2>

<table class="table">
    <thead>
    <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Role</th>
        <th>Gender</th>
        <th>Password</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Username</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>@user.Role</td>
            <td>@user.Gender</td>
            <td>@user.Password</td>
        </tr>
    }
    </tbody>
</table>
<button type="button" class="btn btn-outline-danger">Danger</button>
